//Multiply two numbers
import java.io.*;
import java.util.Scanner;
public class mul
{
        public static void main(String args[])
        {
                Scanner obj = new Scanner(System.in);
                System.out.print("Enter a :");
                int a = obj.nextInt();
                System.out.print("Enter b :");
                int b = obj.nextInt();
                int c = a*b;
                System.out.println("c ="+c);
        }
}
